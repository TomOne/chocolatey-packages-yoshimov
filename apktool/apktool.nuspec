    <copyright></copyright>
    <licenseUrl>http://code.google.com/p/android-apktool/</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <iconUrl>https://github.com/yoshimov/chocolatey-packages/raw/master/apktool/logo.png</iconUrl>
    <dependencies>
      <dependency id="7zip.install" version="" />
    </dependencies>
    <releaseNotes></releaseNotes>
  </metadata>
  <files>
    <file src="tools\**" target="tools" />
    <!--<file src="content\**" target="content" />-->
  </files>
</package>